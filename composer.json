{
	"name": "tatter/http",
	"type": "library",
	"description": "HTTP implementation for CodeIgniter 4",
	"keywords": [
		"codeigniter",
		"codeigniter4",
		"http",
		"message",
		"factory",
		"psr-17",
		"psr-7",
		"psr17",
		"psr7"
	],
	"homepage": "https://github.com/tattersoftware/codeigniter4-http",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthew Gatner",
			"email": "mgatner@tattersoftware.com",
			"homepage": "https://tattersoftware.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.2 || ^8.0",
		"laminas/laminas-diactoros": "^2.5"
	},
	"require-dev": {
		"codeigniter4/codeigniter4": "dev-develop",
		"tatter/tools": "^1.2 || ^2.0"
	},
	"autoload": {
		"psr-4": {
			"Tatter\\Http\\": "src"
		},
		"exclude-from-classmap": [
			"**/Database/Migrations/**"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Support\\": "tests/_support"
		}
	},
	"provide": {
		"psr/http-message-implementation": "1.0",
		"psr/http-factory-implementation": "1.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/codeigniter4/CodeIgniter4"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"analyze": "phpstan analyze",
		"style": "phpcbf --standard=./vendor/codeigniter4/codeigniter4-standard/CodeIgniter4 tests/ src/",
		"test": "phpunit"
	}
}
